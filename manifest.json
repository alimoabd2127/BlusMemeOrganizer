{
  "name": "Blu's Meme Organizer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Organize all your memes and automate image storage",
  "homepage_url": "https://github.com/alimoabd2127",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/options.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/popup.html"
  },
  "permissions": [
    "contextMenus",
    "downloads",
    "storage"
  ]
}